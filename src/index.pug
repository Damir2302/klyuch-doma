                form.form-question
                    .form-bg
                        img(src="assets/images/star.svg", alt="bg")

                    .input-style
                        input(type="text" placeholder="Имя" required)

                    .input-style
                        input(type="tel" placeholder="Телефон" required)

                    .input-style
                        input(type="email" placeholder="Почта" required)

                    .s-form-submit
                        .button-style.button-style--red
